{
  "name": "test-kit",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "prepack": "npm run build",
    "build:yalc": "npm run build && npx yalc publish",
    "dev:yalc": "nodemon -w src -e ts,tsx,js,jsx -d 700ms -x \"npm run build:yalc\""
  },
  "peerDependencies": {
    "@reduxjs/toolkit": ">=1.9.0",
    "@testing-library/react": ">=14.0.0",
    "@testing-library/user-event": ">=13.0.0",
    "@types/jest": ">=29",
    "@types/react": ">=18",
    "react": ">=17 <20",
    "react-dom": ">=17 <20",
    "react-redux": ">=8.0.0",
    "redux": ">=4 <6"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "@types/jest": {
      "optional": true
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "typescript": "^5.9.2",
    "unbuild": "^2.0.0",
    "yalc": "^1.0.0-pre.53"
  }
}
